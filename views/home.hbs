<section id="viewCatalog" class="background-img">
{{#if items}}
    <div class="added-hotels">
  {{#each items}}
      <a href="/details/{{_id}}" class="added-hotel">
        <img src="{{imageUrl}}" alt="" class="picture-added-hotel" />
        <h3>{{name}}</h3>
        <span>Free rooms: {{freeRooms}}</span>
      </a>
  {{/each}}
    </div>
  {{else}}
    <div class="guest">
      There are no Hotels found...
    </div>
    {{/if}}
</section>